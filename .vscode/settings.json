{
  "prettier.enable": false,
  "editor.formatOnSave": false,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "files.associations": {
    "*.css": "postcss"
  },
  "volar.inlayHints.eventArgumentInInlineHandlers": false,
  "cSpell.words": [
    "antfu",
    "daisyui",
    "Nuxt",
    "nuxtjs",
    "supabase",
    "tailwindcss",
    "toastification",
    "userinfo",
    "vuelidate",
    "vueuse"
  ],
  "tailwindCSS.files.exclude": [
    "**/.git/**",
    "**/node_modules/**",
    "**/.hg/**",
    "**/.svn/**",
    "**/.nuxt/**"
  ]

}

